        <training_hello>
            <version>0.1.0</version>
        </training_hello>
    </modules>
    <frontend>
        <routers>
            <training_hello>
                <use>standard</use>
                <args>
                    <module>training_hello</module>
                    <frontName>hellomodule</frontName>
                </args>
            </training_hello>
        </routers>
