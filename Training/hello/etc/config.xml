                <class>training_hello_Model_Resource</class>
            </training_hello_resource>
        </models>
        <blocks>
            <training_hello>
                <class>training_hello_Block</class>
            </training_hello>
        </blocks>
        <helpers>
            <training_hello>
                <class>training_hello_Helper</class>
            </training_hello>
        </helpers>
        <resources>
            <training_hello_setup>
                <setup>
                    <module>training_hello</module>
                </setup>
            </training_hello_setup>
        </resources>
    </global>
</config>